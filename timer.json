{
    "timer": {
        "file": "timer/timer.html",
        "focus_game_takeover": "ReleaseOnHidden",
        "focus_game_takeover_release_hotkey": "timer_showhide",
        "transparent": true,
        "resizable": false,
        "keep_window_location": true,
        "in_game_only": false,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "native_window": true,
        "disable_hardware_acceleration": true,
        "mute": true,
        "size": {
          "width": 300,
          "height": 100
        }
      },
      "timer_showhide": {
        "title": "Show/Hide Boss Timer",
        "action-type": "custom",
        "default": "F12",
        "passthrough": true
      }
}